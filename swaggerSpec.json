      "type": "object",
      "required": [
        "page",
        "size"
      ],
      "properties": {
        "page": {
          "type": "integer",
          "format": "int64"
        },
        "size": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "PageResponse": {
      "type": "object",
      "required": [
        "page",
        "elements"
      ],
      "properties": {
        "page": {
          "type": "object",
          "schema": {
